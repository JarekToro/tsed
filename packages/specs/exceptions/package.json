{
  "name": "@tsed/exceptions",
  "description": "HTTP exceptions module for Ts.ED Framework and other pure Express application",
  "type": "module",
  "version": "8.9.0-beta.7",
  "source": "./src/index.ts",
  "main": "./lib/esm/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "browser": "./lib/browser/exceptions.umd.min.js",
  "exports": {
    ".": {
      "tsed-source": "./src/index.ts",
      "types": "./lib/types/index.d.ts",
      "browser": "./lib/browser/exceptions.umd.min.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn clean && yarn barrels && yarn build:ts && yarn run build:browser",
    "build:browser": "webpack",
    "barrels": "barrels",
    "test": "vitest run",
    "build:ts": "tsc --build tsconfig.esm.json",
    "test:ci": "vitest run --coverage.thresholds.autoUpdate=true"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "statuses": "^2.0.1",
    "tslib": "2.7.0"
  },
  "devDependencies": {
    "@tsed/barrels": "workspace:*",
    "@tsed/typescript": "workspace:*",
    "@types/statuses": "2.0.5",
    "eslint": "9.12.0",
    "typescript": "5.4.5",
    "vitest": "3.1.1",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "@tsed/core": ">=8.9.0-beta.7"
  }
}
